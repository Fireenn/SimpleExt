{
    "manifest_version": 3,
    "name": "Simple Extension",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["https://web.simple-mmo.com/*"],
            "js": ["./scripts/jquery-3.6.1.min.js", "./scripts/common.js"]
        },
        {
            "matches": ["https://web.simple-mmo.com/travel/*"],
            "js": ["./scripts/jquery-3.6.1.min.js", "./scripts/common.js", "./scripts/travel.js"]
        },
        {
            "matches": ["https://web.simple-mmo.com/*/attack/*"],
            "js": ["./scripts/jquery-3.6.1.min.js", "./scripts/common.js", "./scripts/attack.js"]
        },
        {
            "matches": ["https://web.simple-mmo.com/*/gather/*"],
            "js": ["./scripts/jquery-3.6.1.min.js", "./scripts/common.js", "./scripts/gather.js"]
        },
        {
            "matches": ["https://web.simple-mmo.com/inventory/items*"],
            "js": ["./scripts/jquery-3.6.1.min.js", "./scripts/common.js", "./scripts/inventory.js"]
        },
        {
            "matches": ["https://web.simple-mmo.com/user/character"],
            "js": ["./scripts/jquery-3.6.1.min.js", "./scripts/common.js", "./scripts/character_data.js"]
        }
    ]
}